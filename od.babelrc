{
  "presets": [
    ["es2015", { "loose": true }],
    "stage-0"
  ],
  "plugins": [
    [
      "transform-runtime", {
        "helpers": true,
        "polyfill": false,
        "regenerator": false,
        "moduleName": "babel-runtime"
      }
    ],
    "transform-class-properties",
    "add-module-exports"
  ],
  "env": {
    "commonjs": {
      "plugins": [
        ["transform-es2015-modules-commonjs", { "loose": true }]
      ]
    },
    "es": {
      "plugins": [
      ]
    }
  }
}
